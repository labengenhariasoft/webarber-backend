{
  "name": "webarber-backend",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "first": "install",
    "dev": "nodemon .src/server.js --ignore __tests__",
    "pretest": "sequelize db:migrate && sequelize db:seed:all",
    "test": "jest",
    "posttest": "sequelize db:migrate:undo:all && sequelize-cli db:seed:undo:all",
    "prestart": "sequelize db:migrate && db:seed:all",
    "start": "node ./src/server.js",
    "heroku-postbuild": "sequelize db:migrate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "supertest": "^4.0.2"
  }
}
